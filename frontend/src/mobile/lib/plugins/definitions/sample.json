{
    "version": 1.0,
    "type": "Sample plugin",
    "description": "Sample plugin definition",
    "modules": [
        {
            "id": "button",
            "type": "@iota/core",
            "component": "Button",
            "content": "Generate Address",
            "click": {
                "call": "getAddress",
                "parameters": "{{ index }}",
                "assignTo": "address"
            },
            "children": [
                {
                    "id": "text",
                    "type": "@iota/core",
                    "component": "Text",
                    "content": "Child"
                }
            ]
        },
        {
            "id": "text",
            "type": "@iota/core",
            "component": "Text",
            "content": "Random text"
        }
    ]
}