{
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "arrowFunctions": true
        }
    },
    "plugins": [
        "svelte3",
        "babel"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "processor": "svelte3/svelte3"
        }
    ],
    "rules": {
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
        "no-dupe-keys": "error",
        "no-duplicate-imports": "error",
        "no-unneeded-ternary": "warn",
        "no-unreachable": "error",
        "no-var": "error",
        "svelte3/ignore-styles": 0
    }
}
