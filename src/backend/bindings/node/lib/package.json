{
  "name": "api-wrapper",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "yarn build",
    "build": "rimraf ./index.js && rollup -c --silent && yarn generate-declaration",
    "generate-declaration": "tsc src/index.ts --declaration --emitDeclarationOnly --outDir . && yarn cleanup-declaration",
    "cleanup-declaration": "rm -R api-wrapper && mv bindings/node/lib/src/index.d.ts index.d.ts && rm -R bindings"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-typescript": "7.10.4",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "rimraf": "^3.0.2",
    "rollup": "2.28.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.27.2",
    "typescript": "^4.0.3"
  }
}